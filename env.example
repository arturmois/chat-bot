# Configurações do servidor
PORT=3000
NODE_ENV=development

# Configurações do ChatGuru
CHATGURU_API_URL=https://app.zap.guru/api/v1
CHATGURU_API_KEY=your_api_key_here
CHATGURU_ACCOUNT_ID=your_account_id_here
CHATGURU_PHONE_ID=your_phone_id_here

# Configurações do Twilio
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_PHONE_NUMBER=your_phone_number_here
TWILIO_TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Configurações do banco Firebird
FIREBIRD_HOST=localhost
FIREBIRD_PORT=3050
FIREBIRD_DATABASE=/path/to/database.fdb
FIREBIRD_USER=sysdba
FIREBIRD_PASSWORD=masterkey

# Configurações de email
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password
EMAIL_FROM=your_email@gmail.com

# Configurações de Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Configurações de sessão
SESSION_TIMEOUT_MS=1800000 